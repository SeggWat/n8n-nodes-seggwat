{
	"name": "n8n-nodes-seggwat",
	"version": "1.2.0",
	"description": "n8n community nodes for SeggWat feedback and rating management",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"seggwat",
		"feedback",
		"rating",
		"customer-feedback"
	],
	"license": "MIT",
	"homepage": "https://seggwat.com",
	"author": {
		"name": "SeggWat",
		"email": "info@seggwat.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/SeggWat/n8n-nodes-seggwat.git"
	},
	"engines": {
		"node": ">=18.10",
		"pnpm": ">=9.1"
	},
	"packageManager": "pnpm@9.1.4",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc && node scripts/copy-icons.js",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials --ext .ts --fix",
		"lintcheck": "eslint nodes credentials --ext .ts",
		"prepublishOnly": "pnpm build"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/SeggwatApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/Seggwat/Seggwat.node.js"
		]
	},
	"devDependencies": {
		"@types/node": "^25.0.1",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"eslint": "^8.56.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.1",
		"n8n-workflow": "^2.0.1",
		"prettier": "^3.2.0",
		"typescript": "^5.3.0"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
